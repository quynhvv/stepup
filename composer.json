{
    "name": "stepupcarreers",
    "description": "Stepup Carreers",
    "keywords": ["yii2", "stepupcarreers"],
    "homepage": "http://stepupcarreers.com",
    "type": "project",
    "license": "MIT",

    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-imagine": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "yiisoft/yii2-jui": "*",
        "yiisoft/yii2-mongodb": "*",
        "yiisoft/yii2-redis": "*",
        "yiisoft/yii2-authclient": "*",
        "yiisoft/yii2-elasticsearch": "*",
        "letyii/yii2-gii": "dev-master",
        "letyii/yii2-rbac-mongodb": "dev-master",
        "letyii/yii2-mongo-nested-set": "dev-master",
        "letyii/yii2-tinymce": "*",
        "letyii/yii2-jstree": "1.0-alpha",
        "letyii/yii2-cropper": "dev-master",
        "letyii/yii2-imagecache": "dev-master",
        "letyii/yii2-diy": "1.0-alpha",
        "kartik-v/bootstrap-fileinput": "*",
        "kartik-v/dependent-dropdown": "*",
        "kartik-v/yii2-widgets": "*",
        "kartik-v/yii2-icons": "dev-master",
        "kartik-v/yii2-slider": "*",
        "kartik-v/yii2-money": "*",
        "kartik-v/yii2-editable": "*",
        "kartik-v/yii2-popover-x": "*",
        "kartik-v/bootstrap-popover-x": "*",
        "kartik-v/yii2-dynagrid": "*",
        "kartik-v/yii2-checkbox-x": "*",
        "kartik-v/yii2-social": "*",
        "kartik-v/yii2-field-range": "*",
        "kartik-v/yii2-mpdf": "*",
        "kartik-v/yii2-export": "*",
        "kartik-v/yii2-date-range": "*",
        "kartik-v/bootstrap-checkbox-x": "*",
        "kartik-v/yii2-datecontrol": "*",
        "kartik-v/yii2-widget-rating": "*",
        "marciocamello/yii2-paypal": "dev-master"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        },
        "writable": [
            "runtime",
            "assets"
        ],
        "executable": [
            "yii"
        ]
    }
}
